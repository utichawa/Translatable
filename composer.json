{
    "name": "utichawa/translatable",
    "description": "A Laravel package for multilingual models",
    "keywords": ["laravel", "translation", "language", "database"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mechiche Yassine",
            "email": "mechicheyassine@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/utichawa/Translatable/issues"
    },
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.6.*|5.7.*|5.8.*|6.0.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.6.*|3.7.*|3.8.*"
    },
    "autoload": {
        "psr-4": {
            "Utichawa\\Translatable\\": "src/Translatable/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Utichawa\\Translatable\\TranslatableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
